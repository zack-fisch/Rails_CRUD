<%= form_for @movie do |f| %>


<h5> ID: <%= @movie.id %> | Title: <%= @movie.title %> | Director: <%= @movie.director %>  </h5>

<%= f.label :title, "New Title"%>
<%= f.text_field :title%>

<%= f.label :director, "New Director" %>
<%= f.text_field :director %>

<%= f.submit "Update Movie"%>

<% end %>